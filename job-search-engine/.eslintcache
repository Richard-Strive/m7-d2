</b>\n          <p className=\"ml-3\">\n            Testing description\n            {/* <div dangerouslySetInnerHTML={{ __html: description }}></div> */}\n          </p>\n        </div>\n      </div>\n    </>\n  );\n}\n\nexport default FavoriteJobs;\n","/Users/dorian/Documents/GitHub/m7-d2/job-search-engine/src/store/index.js",[],"/Users/dorian/Documents/GitHub/m7-d2/job-search-engine/src/myReducers/index.js",["65"],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"70","severity":1,"message":"71","line":3,"column":42,"nodeType":"72","messageId":"73","endLine":3,"endColumn":46},{"ruleId":"70","severity":1,"message":"74","line":9,"column":3,"nodeType":"72","messageId":"73","endLine":9,"endColumn":6},{"ruleId":"70","severity":1,"message":"75","line":10,"column":3,"nodeType":"72","messageId":"73","endLine":10,"endColumn":14},{"ruleId":"70","severity":1,"message":"76","line":14,"column":3,"nodeType":"72","messageId":"73","endLine":14,"endColumn":6},{"ruleId":"70","severity":1,"message":"77","line":15,"column":3,"nodeType":"72","messageId":"73","endLine":15,"endColumn":10},{"ruleId":"70","severity":1,"message":"78","line":1,"column":15,"nodeType":"72","messageId":"73","endLine":1,"endColumn":19},{"ruleId":"70","severity":1,"message":"79","line":1,"column":21,"nodeType":"72","messageId":"73","endLine":1,"endColumn":30},{"ruleId":"70","severity":1,"message":"77","line":1,"column":32,"nodeType":"72","messageId":"73","endLine":1,"endColumn":39},{"ruleId":"70","severity":1,"message":"80","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":25},{"ruleId":"70","severity":1,"message":"81","line":4,"column":8,"nodeType":"72","messageId":"73","endLine":4,"endColumn":20},{"ruleId":"70","severity":1,"message":"80","line":1,"column":17,"nodeType":"72","messageId":"73","endLine":1,"endColumn":25},{"ruleId":"70","severity":1,"message":"82","line":4,"column":3,"nodeType":"72","messageId":"73","endLine":4,"endColumn":9},{"ruleId":"70"